<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="460.0" prefWidth="507.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.ManageCountriesViewController">
    <children>
        <Label fx:id="countryLabel" layoutX="63.0" layoutY="58.0" text="Country:">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <TextField fx:id="countryTextField" layoutX="132.0" layoutY="55.0" prefHeight="25.0" prefWidth="127.0" />
        <Button fx:id="addCountryButton" layoutX="330.0" layoutY="126.0" mnemonicParsing="false" onAction="#addCountry" prefHeight="29.0" prefWidth="112.0" text="Add Country">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
        <Button fx:id="deleteCountryButton" layoutX="330.0" layoutY="237.0" mnemonicParsing="false" onAction="#deleteCountry" prefHeight="29.0" prefWidth="117.0" text="Delete Country">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
        <Button fx:id="manageFactorsButton" layoutX="330.0" layoutY="296.0" mnemonicParsing="false" onAction="#manageFactors" text="Manage Factors">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
        <Button fx:id="backButton" layoutX="36.0" layoutY="388.0" mnemonicParsing="false" onAction="#back" prefHeight="29.0" prefWidth="53.0" text="Back">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
        <ListView fx:id="listView" layoutX="63.0" layoutY="126.0" prefHeight="200.0" prefWidth="200.0" />
      <Button fx:id="editButton" layoutX="333.0" layoutY="177.0" mnemonicParsing="false" onAction="#editCountry" prefHeight="29.0" prefWidth="112.0" text="Edit Country">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
